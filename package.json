  "description": "Stream AI chat responses in Next.js with TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsc",
    "prepublishOnly": "pnpm build"
  },
  "exports": {
