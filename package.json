  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "next": "^15.1.7",
    "openai": "^4.85.2",
    "react": "^19.0.0",
    "uuid": "^11.1.0"
